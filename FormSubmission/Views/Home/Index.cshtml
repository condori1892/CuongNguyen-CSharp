@{
    ViewData["Title"] = "Home Page";
}
@model Users

<fieldset>
    <legend>Enter your information:</legend>
        <form asp-action="Submit" asp-controller="Home" method="post">
            <div class="form-group">
                <span asp-validation-for="first_name"></span>
                <label asp-for="first_name"></label>
                <input asp-for="first_name" class="form-control">
            </div>  

            <div class="form-group">
                <span asp-validation-for="last_name"></span>
                <label asp-for="last_name"></label>
                <input asp-for="last_name" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="age"></span>
                <label asp-for="age"></label>
                <input asp-for="age" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="email"></span>
                <label asp-for="email"></label>
                <input asp-for="email" class="form-control">
            </div>

            <div class="form-group">
                <span asp-validation-for="password"></span>
                <label asp-for="password"></label>
                <input asp-for="password" class="form-control">
            </div>

            
            <input type="submit" value="Submit">
              
        </form>
        
    
</fieldset>
