
@model BankAccounts.Models.Account
<div class="container">
            
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Amount</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
        @foreach(var trans in Model.transaction)
        {
            <tr>
                <td>$ @trans.activity</td>
                <td>@trans.activity_date.ToString("MMMM dd yyyy")</td> 
            </tr>
        }
    </tbody>
  </table>
</div>